{
  "rewrites": [
    { "source": "/favicon.ico", "destination": "/favicon.ico" },
    { "source": "/assets/(.*)", "destination": "/assets/$1" },
    { "source": "/images/(.*)", "destination": "/images/$1" },
    { "source": "/data/(.*)", "destination": "/data/$1" },

    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
